<UserControl x:Class="Protego.UserControls.CPU1"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Protego.UserControls"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <ProgressBar x:Name="cpuClockProgressBar" Control.Foreground="Red" RangeBase.Value="{Binding Path=CpuClockProgress,Mode=OneWay}" ValueChanged="cpuClockProgressBar_ValueChanged" Height="30" />
    </Grid>

    <!-- cpuClockProgressBar
    <ProgressBar  Control.Foreground="{DynamicResource RadiographGreenGradientBrush}" RangeBase.Value="{Binding Path=CpuClockProgress,Mode=OneWay}" />
        -->
    
</UserControl>
